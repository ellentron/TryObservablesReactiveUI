<reactiveui:ReactiveWindow 
    x:Class="TryRectiveUIwithFody.MainWindow"
    x:TypeArguments="tryRectiveUIwithFody:MyViewModel"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:tryRectiveUIwithFody="clr-namespace:TryRectiveUIwithFody"
    xmlns:reactiveui="http://reactiveui.net"
    mc:Ignorable="d"
    Title="Try ReactiveUI with Fody" Height="450" Width="800">
    
        <StackPanel Margin="20 0 20 0">
            <TextBox x:Name="TheValueBox" />

            <TextBlock x:Name="TheTextBlock" />

            <Button x:Name="TheTextButton" Content="Click Me !!!" MinHeight="20" />

            <TextBlock x:Name="FirstNameTextBox" />

            <TextBox x:Name="NameTextBox" />

            <TextBlock x:Name="NameTextBlock" />
        </StackPanel>
</reactiveui:ReactiveWindow>
